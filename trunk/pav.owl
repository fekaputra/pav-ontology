<!DOCTYPE rdf:RDF [
    <!-- Other namespaces -->
    <!ENTITY xsd  "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdf  "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY owl  "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dct  "http://dublincore.org/documents/dcmi-terms/" >
    <!ENTITY foaf "http://xmlns.com/foaf/0.1/">

    <!-- Main namespace -->
    <!ENTITY pav "http://purl.org/swan/pav/" >

]>
<rdf:RDF
	xml:base="&pav;"
	xmlns="&pav;"
    xmlns:rdfs="&rdfs;"
    xmlns:owl="&owl;"
    xmlns:rdf="&rdf;"
    xmlns:xsd="&xsd;"
    xmlns:dct="&dct;"
    xmlns:foaf="&foaf;">
    
	<owl:Ontology rdf:about="&pav;">
		<rdf:type rdf:resource="&owl;Ontology"/>
		<rdfs:label rdf:datatype="&xsd;string">Provenance, Authoring and Versioning v. 2.0</rdfs:label>
		<owl:versionInfo rdf:datatype="&xsd;string">2.0</owl:versionInfo>    
		
		<dct:language>en</dct:language>
		<dct:title xml:lang="en">Provenance, Authoring and Versioning ontology</dct:title>
		<dct:creator rdf:resource="http://www.paolociccarese.info/foaf.rdf#me" />
		<dct:contributor rdf:datatype="&xsd;string">Paolo Ciccarese</dct:contributor>
		
		<dct:publisher rdf:resource="http://swan.mindinformatics.org" />
		<dct:date rdf:datatype="&xsd;string">May 23, 2010</dct:date>
		<dct:format rdf:datatype="&xsd;string">rdf/xml</dct:format>
	</owl:Ontology>
    
	<!-- 
	///////////////////////////////////////////////////////////////////////////////////////
	//
	// Annotation Properties
	//
	///////////////////////////////////////////////////////////////////////////////////////
	 -->

  	<rdf:Description rdf:about="&dct;title">
		<rdf:type rdf:resource="&owl;AnnotationProperty"/>
  	</rdf:Description>
  	<rdf:Description rdf:about="&dct;description">
		<rdf:type rdf:resource="&owl;AnnotationProperty"/>
  	</rdf:Description>
  	<rdf:Description rdf:about="&dct;date">
		<rdf:type rdf:resource="&owl;AnnotationProperty"/>
 	</rdf:Description>
  	<rdf:Description rdf:about="&dct;creator">
		<rdf:type rdf:resource="&owl;AnnotationProperty"/>
  	</rdf:Description>
  	<rdf:Description rdf:about="&dct;format">
		<rdf:type rdf:resource="&owl;AnnotationProperty"/>
  	</rdf:Description>
  	<rdf:Description rdf:about="&dct;language">
		<rdf:type rdf:resource="&owl;AnnotationProperty"/>
  	</rdf:Description>
  	<rdf:Description rdf:about="&dct;publisher">
		<rdf:type rdf:resource="&owl;AnnotationProperty"/>
  	</rdf:Description>
  	<rdf:Description rdf:about="&dct;contributor">
		<rdf:type rdf:resource="&owl;AnnotationProperty"/>
  	</rdf:Description>
	
	<!-- 
	///////////////////////////////////////////////////////////////////////////////////////
	//
	// PAV Ontology Classes
	//
	///////////////////////////////////////////////////////////////////////////////////////
	 -->
	<owl:Class rdf:about="Curation" rdfs:label="Curation">
	
		<rdfs:comment rdf:datatype="&xsd;string">
		</rdfs:comment>
		
		<!-- created by -->
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&pav;createdBy"/>
				<owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>	
		
		<!-- created on -->
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&pav;createdOn"/>
				<owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
	    
		<!-- curates -->
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="curates"/>
				<owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
	</owl:Class>
	
	
	<!-- 
	///////////////////////////////////////////////////////////////////////////////////////
	//
	// PAV Ontology Properties
	//
	///////////////////////////////////////////////////////////////////////////////////////
	 -->
     
	<owl:DatatypeProperty rdf:about="createdOn">
		<rdfs:comment rdf:datatype="&xsd;string">
			createdOn - The date of creation of the resource.
		</rdfs:comment>
		<rdfs:range rdf:resource="&xsd;dateTime"/>
	</owl:DatatypeProperty>	
	
	<owl:DatatypeProperty rdf:about="lastUpdateOn">
		<rdfs:comment rdf:datatype="&xsd;string">
			lastUpdateOn - The date of the last update of the resource.
		</rdfs:comment>
		<rdfs:range rdf:resource="&xsd;dateTime"/>
	</owl:DatatypeProperty>	
	
	<owl:DatatypeProperty rdf:about="submittedOn">
		<rdfs:comment rdf:datatype="&xsd;string">
			submittedOn - Date of submission of the resource. Examples of resources to which a Date 
			Submitted may be relevant are a thesis (submitted to a university department) or an article 
			(submitted to a journal) or a discourse element in the case of swan scientific discourse.
		</rdfs:comment>
		<rdfs:range rdf:resource="&xsd;dateTime"/>
	</owl:DatatypeProperty>	
	
	<owl:DatatypeProperty rdf:about="acceptedOn">
		<rdfs:comment rdf:datatype="&xsd;string">
			acceptedOn - Date of acceptance of the resource. Examples of resources to which a Date 
			Accepted may be relevant are a thesis (accepted by a university department) or an article 
			(accepted by a journal) or a discourse element in the case of swan scientific discourse.
		</rdfs:comment>
		<rdfs:range rdf:resource="&xsd;dateTime"/>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="publishedOn">
		<rdfs:comment rdf:datatype="&xsd;string">
			publishedOn - The date of publishing.</rdfs:comment>
		<rdfs:range rdf:resource="&xsd;dateTime"/>
	</owl:DatatypeProperty>
     
	<!-- Data Import --> 
	
	<owl:DatatypeProperty rdf:about="importedOn">
		<rdfs:comment rdf:datatype="&xsd;string">
			importedOn - The date of the import of the resource.
		</rdfs:comment>
		<rdfs:range rdf:resource="&xsd;dateTime"/>
	</owl:DatatypeProperty>

	<owl:DatatypeProperty rdf:about="importedFirstOn">
		<rdfs:subPropertyOf rdf:resource="importedOn"/>
		<rdfs:comment rdf:datatype="&xsd;string">
			lastImportedOn - The date of the first import of the resource. This property
			is used when the history of the versions of the different imports is not stored.
		</rdfs:comment>
		<rdfs:range rdf:resource="&xsd;dateTime"/>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="importedLastOn">
		<rdfs:subPropertyOf rdf:resource="importedOn"/>
		<rdfs:comment rdf:datatype="&xsd;string">
			lastImportedOn - The date of the last import of the resource. This property
			is used when the history of the versions of the different imports is not stored.
		</rdfs:comment>
		<rdfs:range rdf:resource="&xsd;dateTime"/>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="importedWithId">
		<rdfs:comment rdf:datatype="&xsd;string">
			sourceIdentifier - The unique identifier of the encoded information in the original source.
		</rdfs:comment>
	</owl:DatatypeProperty>
	
	<!-- Source access -->
	
	<owl:DatatypeProperty rdf:about="sourceAccessedOn">
		<rdfs:comment rdf:datatype="&xsd;string">
			sourceAccessedOn - The date when the original source has been accessed to create the resource.
		</rdfs:comment>
		<rdfs:range rdf:resource="&xsd;dateTime"/>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="sourceFirstAccessedOn">
		<rdfs:subPropertyOf rdf:resource="sourceAccessedOn"/>
		<rdfs:comment rdf:datatype="&xsd;string">
			sourceFirstAccessedOn - The date when the original source has been first accessed and verified.
		</rdfs:comment>
		<rdfs:range rdf:resource="&xsd;dateTime"/>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="sourceLastAccessedOn">
		<rdfs:subPropertyOf rdf:resource="sourceAccessedOn"/>
		<rdfs:comment rdf:datatype="&xsd;string">
			sourceLastAccessedOn - The date when the original source has been last accessed and verified.
		</rdfs:comment>
		<rdfs:range rdf:resource="&xsd;dateTime"/>
	</owl:DatatypeProperty>
     
	<!-- Versioning -->
	<owl:DatatypeProperty rdf:about="versionNumber">
		<rdfs:comment rdf:datatype="&xsd;string">version - The version of the resource.</rdfs:comment>
		<rdfs:range rdf:resource="&xsd;string"/>
	</owl:DatatypeProperty>	

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // PAV Ontology Relationships
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->	
     
    <!-- Authoring -->  
    
	<owl:ObjectProperty rdf:about="createdBy">
		<rdfs:label xml:lang="en">Created By</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">
			createdBy - An entity primary responsible for making the resource.
		</rdfs:comment>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="contributedBy">
		<rdfs:label xml:lang="en">Contributed By</rdfs:label>
	</owl:ObjectProperty>   
	 
	<owl:ObjectProperty rdf:about="authoredBy">
		<rdfs:label xml:lang="en">Authored By</rdfs:label>
		<rdfs:subPropertyOf rdf:resource="contributedBy"/>
	</owl:ObjectProperty>  
	
	<owl:ObjectProperty rdf:about="curatedBy">
		<rdfs:label xml:lang="en">Curated By</rdfs:label>
		<rdfs:subPropertyOf rdf:resource="contributedBy"/>
		<rdfs:comment rdf:datatype="&xsd;string">
			curatedBy - An entity primary responsible for verifying the quality of the resource.
		</rdfs:comment>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="contributors">
		<rdfs:label xml:lang="en">Contributors</rdfs:label>
	</owl:ObjectProperty>   
	 
	<owl:ObjectProperty rdf:about="authors">
		<rdfs:label xml:lang="en">Authors</rdfs:label>
		<rdfs:subPropertyOf rdf:resource="contributors"/>
	</owl:ObjectProperty>  
	
	<owl:ObjectProperty rdf:about="curators">
		<rdfs:label xml:lang="en">Curators</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">
			curators - A list of entities primary responsible for verifying the quality of the resource.
		</rdfs:comment>
		<rdfs:subPropertyOf rdf:resource="contributors"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="submittedBy">
		<rdfs:label xml:lang="en">Submitted By</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">submittedBy - </rdfs:comment>
	</owl:ObjectProperty>	
	
	<owl:ObjectProperty rdf:about="publishedBy">
		<rdfs:label xml:lang="en">Published By</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">
			publishedBy - An entity responsible for making the resource available.
		</rdfs:comment>
	</owl:ObjectProperty>
    
	<!-- Data Import --> 
	
	<owl:ObjectProperty rdf:about="importedFromSource">
		<rdfs:label xml:lang="en">Imported From Source</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">
			Identify the original source of the imported information.
		</rdfs:comment>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="importedBy">
		<rdfs:label xml:lang="en">Imported By</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">
			importedBy - An entity responsible for importing the data from an external source.
			Usually a software entity.
		</rdfs:comment>
	</owl:ObjectProperty>
     
	<!-- Curation -->      
	
	<owl:ObjectProperty rdf:about="curates">
		<rdfs:label xml:lang="en">Curates</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">
			curates - 
		</rdfs:comment>
		<rdfs:domain rdf:resource="Curation" />
		<owl:inverseOf rdf:resource="hasCuration"/>
	</owl:ObjectProperty> 
	
	<owl:ObjectProperty rdf:about="hasCuration">
		<rdfs:label xml:lang="en">Has Curation</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">
			hasCuration - 
		</rdfs:comment>
		<rdfs:range rdf:resource="Curation" />
	</owl:ObjectProperty>  
	
	<!-- Versioning -->
	
	<owl:TransitiveProperty rdf:about="earlierVersion">
		<rdfs:label xml:lang="en">Earlier Version</rdfs:label>
	  	<rdfs:comment rdf:datatype="&xsd;string">earlierVersion - An earlier version of the resource.</rdfs:comment>
	  	<rdfs:seeAlso rdf:resource="http://rdfs.org/sioc/ns#earlier_version"/>
      	<owl:inverseOf rdf:resource="laterVersion"/>
	</owl:TransitiveProperty>
	
	<owl:ObjectProperty rdf:about="previousVersion">
		<rdfs:label xml:lang="en">Previous Version</rdfs:label>
	  	<rdfs:comment rdf:datatype="&xsd;string">previousVersion - The previous version of the resource.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="earlierVersion"/>
        <rdfs:seeAlso rdf:resource="http://rdfs.org/sioc/ns#previous_version"/>
        <owl:inverseOf rdf:resource="nextVersion"/>
	</owl:ObjectProperty>
	
	<owl:TransitiveProperty rdf:about="laterVersion">
		<rdfs:label xml:lang="en">Later Version</rdfs:label>
	  	<rdfs:comment rdf:datatype="&xsd;string">laterVersion - A later version of the resource.</rdfs:comment>
	  	<rdfs:seeAlso rdf:resource="http://rdfs.org/sioc/ns#later_version"/>
      	<owl:inverseOf rdf:resource="earlierVersion"/>
	</owl:TransitiveProperty>
	
	<owl:ObjectProperty rdf:about="nextVersion">
		<rdfs:label xml:lang="en">Next Version</rdfs:label>
	  	<rdfs:comment rdf:datatype="&xsd;string">nextVersion - The next version of the resource.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="laterVersion"/>
        <rdfs:seeAlso rdf:resource="http://rdfs.org/sioc/ns#next_version"/>
        <owl:inverseOf rdf:resource="previousVersion"/>
	</owl:ObjectProperty>
	
	<!-- Evolution-->
	
	<owl:ObjectProperty rdf:about="derivedFrom">
		<rdfs:label xml:lang="en">Derived From</rdfs:label>
	  	<rdfs:comment rdf:datatype="&xsd;string">
	  	    derivedFrom - The resource from which this entity is derived from. 
	  		This relationship is particularly useful when the goal is not to create a new version of a resource
	  		but to create a different resource derived from the original that keeps its own identity and revision
	  		path. The new resource is not intended to substitute the original one.
	  	</rdfs:comment>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="evolvedFrom">
		<rdfs:label xml:lang="en">Evolved From</rdfs:label>
	  	<rdfs:comment rdf:datatype="&xsd;string">
	  	    evolvedFrom - The resource from which this entity is evolving from. 
	  		This relationship is particularly useful when the goal is not to create a new version of a resource
	  		but to create a different resource derived from the original that keeps its own identity and revision
	  		path. The new resource is not intended to substitute the original one by the same agent (or somebody
	  		with equal rights) that created the previous resource. Same evolution path of the first resource.
	  	</rdfs:comment>
	  	<rdfs:subPropertyOf rdf:resource="derivedFrom"/>
	</owl:ObjectProperty>
	
	<owl:SymmetricProperty rdf:about="variantOf">
		<rdfs:label xml:lang="en">Variant Of</rdfs:label>
	  	<rdfs:comment rdf:datatype="&xsd;string">
	  	    variantOf - The relationship connects two entities that are two alternative evolutions
	  	    of the same initial entity.    
	  	</rdfs:comment>
	</owl:SymmetricProperty>
	
</rdf:RDF>